<h2 class="ui header">
  <img class="ui image" src={{contextPath}}"/images/icons/school.png">
  <div class="content" style="margin-top: 10px;">
    Top 50 records:
  </div>
</h2>
<div>
     <table class="ui selectable striped celled tiny table">
         <tbody>
             {{#each task_running_result}}
             <tr>
                <td>{{lineNumber}}</td>
                <td>{{file.originalName}}</td>
                <td>{{content}}</td>
             </tr>
             {{/each}}
         </tbody>
     </table>
</div>
